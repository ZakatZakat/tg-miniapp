{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "strict": true,
        "noEmit": true,
        "isolatedModules": true,
        "allowJs": false,
        "types": [
            "vite/client",
            "react",
            "react-dom"
        ]
    },
    "include": [
        "src"
    ]
}